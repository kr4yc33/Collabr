    <h1>Your Projects</h1>
    <% items.forEach(item =>{%>
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">
                <%=item.name%>
            </h4>
            <div class="card-subtitle text-muted">
                <%=item.usercreatorid%>
            </div>
            <div class="card-content">
                <div class="col-12">
                    <%=item.description%>
                </div>
                <div class="col-12">
                    <%=item.type%>
                </div>
                <div class="col-12">
                    <%=item.status%>
                </div>
            </div>
            <div class="row">
                <form action="/projects/yourprojects/delete/<%= item.id %>?_method=DELETE" method="POST">
                    <button type="submit" class="btn btn-danger">Delete</button>
                </form>
            </div>
        </div>

        <div class="container">
            <div class="row">
                <form action="/projects/yourprojects/addpost/<%=item.id %>" method="POST">
                    <div class="col-12">
                        <label>post title</label>
                        <input type="text" name="title">
                    </div>
                    <div class="col-12">
                        <label>description</label>
                        <input type="textarea" name="description">    
                    </div>
                    <div class="col-12">
                        <label>assetname</label>
                        <input type="textarea" name="assetname">    
                    </div>
                    <div class="col-12">
                        <label>asset description</label>
                        <input type="textarea" name="assetdescription">    
                    </div>
                    <div class="col-12">
                        <label>assetlink</label>
                        <input type="textarea" name="assetlink">    
                    </div>
                    
                    <button type="submit" class="btn btn-primary">post</button>
                
                </form>        
            </div>
            
            <div class="row">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">
                         post title
                        </h4>
                        <div class="card-subtitle text-muted">
                          post by
                        </div>
                        <div class="card-content">
                                post content
                        </div>
                        </div>
                </div>
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">
                       asset name
                        </h4>
                        <div class="card-subtitle text-muted">
                          post by
                        </div>
                        <div class="card-content">
                            asset description
                        </div>
                        <div class="card-content">
                            asset link
                        </div>
                        </div>
                </div>
            </div>
        
        </div>

        
    </div>
    <% })%>
